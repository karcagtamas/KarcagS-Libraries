<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <OutputType>Library</OutputType>
        <IsPackable>true</IsPackable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>4.2.2</PackageVersion>
        <Title>KarcagS Client Common</Title>
        <AssemblyVersion>4.2.2</AssemblyVersion>
        <FileVersion>4.2.2</FileVersion>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\KarcagS.Http\KarcagS.Http.csproj" />
        <ProjectReference Include="..\KarcagS.Shared\KarcagS.Shared.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0" />
    </ItemGroup>

</Project>
